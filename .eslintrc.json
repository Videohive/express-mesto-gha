{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": ["**/*.{test,spec}.{js,jsx,ts,tsx}", "**/webpack.config.js", "**/webpack.config.*.js"]
      }
    ]
  }
}
